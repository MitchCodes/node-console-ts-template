{
  "name": "node-console-ts-template",
  "version": "0.0.1",
  "description": "Template for a Node.JS application using TypeScript.",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "main": "",
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/nconf": "~0.0.37",
    "@types/node": "^12.12.53",
    "file-loader": "~6.2.0",
    "jest": "~26.1.0",
    "merge-jsons-webpack-plugin": "~2.0.0-alpha",
    "ts-jest": "^26.1.4",
    "ts-loader": "~8.1.0",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "~6.2.0",
    "tsutils": "~3.17.1",
    "typescript": "~3.9.7",
    "webpack": "~5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "~5.7.3"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp logs",
    "build": "webpack --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
    "build-npm": "tsc --project tsconfig.release.json",
    "watch": "webpack --config webpack.dev.js --watch",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "lint-autofix": "tslint -t stylish --fix --project \"tsconfig.json\"",
    "pretest": "npm run lint --force",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "test-basic": "jest",
    "test-example": "jest --rootDir=./ --config=./jest/jest.example.config.js",
    "start": "node ./build/main.js"
  },
  "author": "Mitch Chiles <mitchchiles@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MitchCodes/node-console-ts-template.git"
  },
  "bugs": {
    "url": "https://github.com/MitchCodes/node-console-ts-template.git/issues"
  },
  "homepage": "https://github.com/MitchCodes/node-console-ts-template",
  "dependencies": {
    "applicationinsights": "1.3.1",
    "moment": "^2.21.0",
    "nconf": "~0.10.0",
    "tslib": "~2.0.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.8.0",
    "winston-transport": "4.3.0",
    "tsdatautils-core": "~0.0.95",
    "tsdatautils-memorycache": "~0.0.7",
    "tsdatautils-azuretablestorage": "~0.0.7"
  }
}
